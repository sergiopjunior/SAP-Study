<mvc:View
	controllerName="consultaprodutos.controller.MaterialHandlingList"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<List id="MaterialHandlingList" binding="{/MaterialHandlingList}">
		<headerToolbar>
			<OverflowToolbar>
				<Title text="Movimentação de Materiais" level="H2" />
				<!--
				<ToolbarSpacer />
				<Text id="product_count" text="Total: {/ProductCollectionStats/Counts/Total}"/>
				-->
				<Button
					icon="sap-icon://settings"
					press="handleButtonPress" />
			</OverflowToolbar>
		</headerToolbar>
		<StandardListItem
			title="{0/Name}"
			description="{0/ProductId}"
			icon="{0/ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			type="Navigation" />
	</List>
</mvc:View>